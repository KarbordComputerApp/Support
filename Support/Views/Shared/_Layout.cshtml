    <div id="box-chat" class="fadeInUp">
        <div class="box-header">
            <div id="btn-max-chat" title="تغییر اندازه">
                <img id="ChatSizeIcon" src="" width="18" />
            </div>

            <div id="btn-end-chat" title="پایان یافته">
                <img src="~/Content/img/Icon_Blue/stop.png" width="14" />
            </div>
            <div id="btn-close-chat" title="بستن">
                <svg fill="#082b44" height="18" viewBox="0 0 24 24" width="18" xmlns="http://www.w3.org/2000/svg">
                    <path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"></path>
                </svg>
            </div>
            <div class="row">
                <img src="~/Content/img/LogoMin.png" alt="avatar" width="40" height="40">
                <p id="L_MotaghaziChat" style="font-size: 15px;padding-right: 10px;text-wrap: balance;width: 300px;position: relative;top: 10px;">کارشناس پشتیبانی</p>
            </div>

        </div>


        <div id="chatbox"></div>


        <div id="box-send" class="hasmic box-footer">
            <input id="motaghaziChat" type="text" style="font-size:14px;" placeholder="نام درخواست کننده">
            <div class="row" id="ActiveAttach" >
                <input id="ActiveAttachChat" type="checkbox" style="width: 16px;margin-left: 5px;margin-bottom: 0px;margin-right: 5px;" />
                <label for="ActiveAttachChat" style="margin-top:7px;margin-bottom:0px">فعال کردن ارسال فایل</label>
            </div>

            <div class="form-group is-empty">
                <textarea maxlength="2000" id="ChatMessage" type="text" placeholder="پیامی بنویسید..." class="form-controlChat" style="overflow: hidden; overflow-wrap: break-word; height: 40px;"></textarea>
                <span class="material-input"></span>
            </div>


            <div id="ChatAttach" class="fix-contentchat" style="margin-top:10px">
                <a class="btn btn-simple btn-file" id="P_AttachChat" style="box-shadow: none;top: 5px;">
                    <span class="fileinput-new">
                        <svg fill="#999999" height="23" viewBox="0 0 24 24" width="23" xmlns="http://www.w3.org/2000/svg">
                            <path d="M16.5 6v11.5c0 2.21-1.79 4-4 4s-4-1.79-4-4V5c0-1.38 1.12-2.5 2.5-2.5s2.5 1.12 2.5 2.5v10.5c0 .55-.45 1-1 1s-1-.45-1-1V6H10v9.5c0 1.38 1.12 2.5 2.5 2.5s2.5-1.12 2.5-2.5V5c0-2.21-1.79-4-4-4S7 2.79 7 5v12.5c0 3.04 2.46 5.5 5.5 5.5s5.5-2.46 5.5-5.5V6h-1.5z"></path>
                        </svg>
                    </span>
                    <input id="upload-input" type="file" name="uploads[]" multiple="">
                </a>

                <a id="ChatSend" class="btn btn-simple btn-send" style="display: inline-block;box-shadow: none;">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="23" height="23" fill="#999999">
                        <path d="M22,3,1,12l21,9V14L7,12l15-2Z"></path>
                    </svg>
                </a>
            </div>
        </div>

    </div>

