<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <link rel="icon" type="image/png" href="/Content/img/LogoTop.png">
    <title>کاربرد کامپیوتر</title>
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/materialize.min.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/all-themes.css" rel="stylesheet" />
    <link href="~/Content/css/site.css" rel="stylesheet" />
    <link href="~/Content/css/stylelogin.css" rel="stylesheet" />
    <script type="text/javascript">
        // window.history.forward();
        // function noBack() {
        //      window.history.forward();
        //  }
    </script>
</head>
<body ondragstart="return false;" ondrop="return false;">

    <div id="loadingsite" class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30">
                <img class="loading-img-spin" src="~/Content/loading1.png" width="20" height="20" alt="admin">
            </div>
            <p dir="rtl" data-bind="text:'لطفا منتظر بمانید'"></p>
        </div>
    </div>

    <aside id="leftsidebar" class="sidebar" hidden>
        <div class="menu">
            <ul class="list">
                <li class="sidebar-user-panel active">
                </li>

            </ul>
        </div>
    </aside>



    <div class="row">
        <div class="col-lg-6 PanelLeftLogin d-none d-lg-block">
            <div class="row" style="text-align:center;">
                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                </div>

                <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
                    <img id="LogoLogin" src="~/Content/img/Logo.png" style="padding: 35% 20% 0% 20%; max-width:100%" />

                    <div style="padding: 35% 20% 0% 20%;">
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 " id="datalogin">

            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-2 col-xs-2">
                </div>

                <div class="col-lg-6 col-md-6 col-sm-8 col-xs-8">

                    @RenderBody();

                </div>
            </div>
        </div>
    </div>

    <div class="row" style="text-align:center;">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <p style="color: #fefefe;" data-bind="text:'کلیه حقوق مادی و معنوی این سایت متعلق به شرکت مهندسی کاربرد کامپیوتر میباشد'">

            </p>
        </div>
    </div>




    <script type="text/javascript">
        sessionStorage.ver = @(Support.Controllers.Unit.UnitPublic.titleVerNumber);
        localStorage.setItem("urlIndex", '@Url.Action("Index", "Home")');
        localStorage.setItem("urlChangePassword", '@Url.Action("ChangePassword", "Home")');
        localStorage.setItem("urlRecoveryPassword", '@Url.Action("RecoveryPassword", "Home")');
        localStorage.setItem("urlLogin", '@Url.Action("Login", "Home")');
    </script>
    <script src="~/Content/js/app.min.js"></script>
    <script src="~/Content/js/admin.js"></script>
    <script src="~/Content/js/knockout-3.5.0.js"></script>
    <script src="~/Content/js/sweet2.js"></script>
    <script src="~/Scripts/app.js"></script>
    <script src="~/Content/js/persian-date.min.js?dt=@(Support.Controllers.Unit.UnitPublic.titleVerNumber)"></script>
    <script src="~/Scripts/PublicAFI.js?dt=@(Support.Controllers.Unit.UnitPublic.titleVerNumber)"></script>


    <script src="/Content/js/form.min.js"></script>
    @RenderSection("scripts", false)
</body>
</html>






